            'dc460dee4d5a3685f4bcf7b7d4409e54ce1374569da90b7461c9dc40cddb31ee')

package() {
	cd "$srcdir"
	install -Dm644 geoip.dat "$pkgdir"/usr/share/dae/geoip.dat
